{
  "manifest_version": 2,
  "name": "Flowly",
  "author": "Th√©o Vilain / Teyk0o",
  "version": "1.2.5",
  "license": "All Rights Reserved",
  "description": "Where every word matters",
  "browser_specific_settings": {
    "gecko": {
      "id": "flowlyweb@theovilain.com",
      "strict_min_version": "112.0"
    }
  },
  "permissions": [
    "*://*.oclock.school/*",
    "https://api.theovilain.com/*"
  ],
  "icons": {
    "48": "icons/flowly-48.png",
    "96": "icons/flowly-96.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.oclock.school/*"
      ],
      "js": [
        "**/*.js"
      ],
      "css": [
        "**/*.css"
      ]
    }
  ]
}