{
  "manifest_version": 3,
  "name": "WWSNB - We Want Slippers Not BBB",
  "author": "Th√©o Vilain / Teyk0o",
  "version": "1.0.1",
  "description": "Retrieve functionality from Slippers for BBB.",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "*://*.oclock.school/*",
    "https://api.theovilain.com/*"
  ],
  "icons": {
    "48": "icons/wwsnb-48.png",
    "96": "icons/wwsnb-96.png"
