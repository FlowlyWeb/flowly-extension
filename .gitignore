lib/
.parcel-cache/
.webpack_cache/

# TypeScript compilation output
*.js
*.js.map
!webpack.config.js
!eslint.config.js
!scripts/release.ts

# Compiled JavaScript from TypeScript (if not in dist/)
**/*.js
**/*.js.map
!node_modules/**/*.js

