  "scripts": {
    "clean": "rimraf dist dist-scripts",
    "build": "webpack --mode=development",
    "build:scripts": "webpack --config webpack.config.js --mode=production --env target=node-scripts",
    "build:prod": "webpack --mode=production",
    "dev": "webpack --watch --mode=development",
    "start": "web-ext run --source-dir ./dist/",
    "release": "node --experimental-vm-modules dist-scripts/release.js",
    "release:no-git": "node --experimental-vm-modules dist-scripts/release.js patch --no-git",
    "release:patch": "node --experimental-vm-modules dist-scripts/release.js patch",
    "release:patch:no-git": "node --experimental-vm-modules dist-scripts/release.js patch --no-git",
    "release:patch:skip-build": "node --experimental-vm-modules dist-scripts/release.js patch --no-git --skip-build",
    "release:minor": "node --experimental-vm-modules dist-scripts/release.js minor",
    "release:minor:no-git": "node --experimental-vm-modules dist-scripts/release.js minor --no-git",
    "release:major": "node --experimental-vm-modules dist-scripts/release.js major",
    "release:major:no-git": "node --experimental-vm-modules dist-scripts/release.js major --no-git",
    "upload-only": "node --experimental-vm-modules dist-scripts/release.js patch --no-git --skip-build --upload-only"
  },
